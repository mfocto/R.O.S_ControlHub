{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "App": "Information",
      "ROS_ControlHub.Adapters.Real": "Debug"
    }
  },
  "StatePolling": {
    "IntervalMs": 500
  },
  "OpcUa": {
    "EndpointUrl": "opc.tcp://localhost:49320",
    "SessionName": "GrpcService",
    "ConnectionTimeoutMs": 10000,
    "SessionTimeoutMs": 1800000,
    "AutoReconnect": true,
    "ReconnectIntervalMs": 5000,
    "SecurityMode": "None",
    "SecurityPolicy": "None",
    "AutoAcceptUntrustedCertificates": true,
    "CertificatePaths": {
      "Own": "c:/auth/pki/own",
      "Trusted": "c:/auth/pki/trusted",
      "Issuers": "c:/auth/pki/issuers",
      "Rejected": "c:/auth/pki/rejected"
    },
    "Kepware": {
      "Channel": "ModbusTCP",
      "Device": "ESP32_01",
      "Tags": {
        "Connected": "Connected",
        "Running": "Running",
        "Speed": "Speed",
        "ControlCommand": "ControlCommand"
      }
    }
  },
  "DeviceTag": {
    "DeviceId": "ESP32_01",
    "ChannelName": "ModbusTCP",
    "DeviceName": "ESP32_01"
  },
  "AllowedHosts": "*"
}
